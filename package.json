{
  "name": "nuxt-express-vuetify-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt.js + Express boilerplate with Vuetify component framework",
  "author": "Danijel Martinek <danijel.martinek.4@gmail.com>",
  "private": true,
  "scripts": {
    "postinstall": "patch -p0 < gritty.patch",
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.3",
    "gritty": "^4.3.8",
    "lodash": "^4.17.19",
    "nuxt": "^2.4.3",
    "nuxt-oauth": "^2.0.2",
    "passport": "^0.4.0",
    "passport-strava": "latest",
    "source-map-support": "^0.5.10",
    "vuetify": "1.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "backpack-core": "^0.8.3",
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
